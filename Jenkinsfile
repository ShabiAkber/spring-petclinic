pipeline {
  agent any
  stages {
    stage('Compile') {
      steps {
        echo 'compile the code'
      }
    }

    stage('Test') {
      steps {
        echo 'testing'
      }
    }

    stage('Install Packages') {
      steps {
        echo 'installing packages into the code'
      }
    }

    stage('Deploy') {
      steps {
        echo 'deploying'
      }
    }

  }
}